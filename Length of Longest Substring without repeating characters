public class longestSubstring()
{
 public static void longestString(String s)
 {
   HashSet<Character> set = new HashSet<Character>();
   int i = 0;
   int j = 0;
   int max = 0;
   while(j < s.length())
   {
      if(!set.contains(s.charAt())
      {
        set.add(s.charAt(j++));
        max = Math.max(max,s.size());
      }else
      {
        set.remove(s.charAt(i++));
      }
   }
  return max;
 }
}
