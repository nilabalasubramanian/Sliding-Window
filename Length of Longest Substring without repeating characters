public class longestSubstring(String s)
{
 public static void longestString()
 {
   HashSet<String> s = new HashSet<String>();
   int i = 0;
   int j = 0;
   int max;
   while(j < s.length())
   {
      if(!set.contains(s.charAt())
      {
        set.add(s.charAt(j++));
        max = Math.max(max,s.size());
      }else
      {
        set.remove(s.charAt(i++));
      }
   }
  return max;
 }
}
